Algoritmo "para de 10 ate 1"

Var

   j,i: inteiro;

Inicio
   escreval("Imprimindo de 10 a 1")
   para j de 10 ate 1 passo j-1 faca
      escreval(j)
   fimpara
   escreval("Imprimindo de 10 a 1")
   para i de 10 ate 1 passo i-2 faca
      escreval(i)
   fimpara
   escreval("Fogo !!!")

Fimalgoritmo