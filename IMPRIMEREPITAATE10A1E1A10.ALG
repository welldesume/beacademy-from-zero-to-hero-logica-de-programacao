Algoritmo "imprimeRepitaAte10a1e1a10"

Var
   j:inteiro;


Inicio
   escreval("Imprimindo de 1 a 10 ate:")
   j<-1
   repita
      escreval(j)
      j <- j+1
   ate j>10
   escreval("Valor do j: ", j)

   escreval("Imprimindo de 10 a 1 ate:")
   j<-10
   repita
      escreval(j)
      j <- j-1
   ate j<1
   escreval("Valor do j: " , j)

Fimalgoritmo