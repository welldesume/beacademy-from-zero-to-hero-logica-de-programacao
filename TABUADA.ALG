Algoritmo "tabuada"


Var
   num1,num2,tab,i:inteiro;


Inicio
   escreva("Digite qual a tabuada você deseja")
   leia(tab)
   escreva("Digite o número inicial da tabuada")
   leia(num1)
   escreva("Digite o número final da tabuada")
   leia(num2)
   para i de num1 ate num2 faca
      escreval(tab," + " , i, " = ", tab+i )
   fimpara


Fimalgoritmo